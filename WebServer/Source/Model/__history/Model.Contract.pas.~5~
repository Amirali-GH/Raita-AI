    [MVCTableField('Is_Contract_Blacklisted')] FIsContractBlacklisted: NullableString;
    [MVCTableField('Is_Customer_Blacklisted')] FIsCustomerBlacklisted: NullableString;
    [MVCTableField('Last_Issued_Invitation')] FLastIssuedInvitation: NullableString;
    [MVCTableField('Last_Completed_Payment_Invitation')] FLastCompletedPaymentInvitation: NullableString;

    [MVCTableField('First_Signature_Date')] FFirstSignatureDate: NullableString;
    [MVCTableField('First_Signatory')] FFirstSignatory: NullableString;
    [MVCTableField('Second_Signature_Date')] FSecondSignatureDate: NullableString;
    [MVCTableField('Second_Signatory')] FSecondSignatory: NullableString;

    [MVCTableField('Customer_Outstanding_Balance')] FCustomerOutstandingBalance: NullableString;
    [MVCTableField('Checks_Count_After_Kardex')] FChecksCountAfterKardex: NullableString;
    [MVCTableField('Scrapping_Certificate_Date')] FScrappingCertificateDate: NullableString;

    [MVCTableField('ContractFileID')] FContractFileID: Int64;

  public
    property ID: Int64 read FID write FID;

    property Number: NullableString read FNumber write FNumber;
    property Date_: NullableString read FDate write FDate;
    property GregorianDate: NullableString read FGregorianDate write FGregorianDate;

    property RequestNumber: NullableString read FRequestNumber write FRequestNumber;
    property RequestDate: NullableString read FRequestDate write FRequestDate;
    property RequestGregorianDate: NullableString read FRequestGregorianDate write FRequestGregorianDate;

    property CustomerID: Int64 read FCustomerID write FCustomerID;
    property DealershipID: Int64 read FDealershipID write FDealershipID;
    property VehicleTypeID: Int64 read FVehicleTypeID write FVehicleTypeID;
    property ColorID: Int64 read FColorID write FColorID;

    property Gateway: NullableString read FGateway write FGateway;
    property Region: NullableString read FRegion write FRegion;
    property SalesLicenseID: Int64 read FSalesLicenseID write FSalesLicenseID;
    property PaymentMethodID: Int64 read FPaymentMethodID write FPaymentMethodID;

    property DeliveryDate: NullableString read FDeliveryDate write FDeliveryDate;
    property DeliveryDueDays: NullableString read FDeliveryDueDays write FDeliveryDueDays;
    property AssignableChassis10thDigit: NullableString read FAssignableChassis10thDigit write FAssignableChassis10thDigit;
    property ChassisNumber: NullableString read FChassisNumber write FChassisNumber;
    property DeliveryMonth: NullableString read FDeliveryMonth write FDeliveryMonth;
    property DeliveryDealershipID: NullableInt64 read FDeliveryDealershipID write FDeliveryDealershipID;
    property DeliveryLocation: NullableString read FDeliveryLocation write FDeliveryLocation;

    property RegistrationDocsStatus: NullableString read FRegistrationDocsStatus write FRegistrationDocsStatus;
    property FinalPaymentDocsStatus: NullableString read FFinalPaymentDocsStatus write FFinalPaymentDocsStatus;
    property InvoiceNotIssuedStatus: NullableString read FInvoiceNotIssuedStatus write FInvoiceNotIssuedStatus;
    property VerificationDocsStatus: NullableString read FVerificationDocsStatus write FVerificationDocsStatus;
    property ChassisStatus: NullableString read FChassisStatus write FChassisStatus;

    property AllocationDate: NullableString read FAllocationDate write FAllocationDate;
    property AllocationGregorianDate: NullableString read FAllocationGregorianDate write FAllocationGregorianDate;
    property PlatingDate: NullableString read FPlatingDate write FPlatingDate;
    property PlatingGregorianDate: NullableString read FPlatingGregorianDate write FPlatingGregorianDate;
    property PlatingGregorianDate1: NullableString read FPlatingGregorianDate1 write FPlatingGregorianDate1;

    property DocumentDate: NullableString read FDocumentDate write FDocumentDate;
    property DocumentStatus: NullableString read FDocumentStatus write FDocumentStatus;
    property PhysicalDeliveryDate: NullableString read FPhysicalDeliveryDate write FPhysicalDeliveryDate;
    property PlateNumber: NullableString read FPlateNumber write FPlateNumber;
    property CancellationDate: NullableString read FCancellationDate write FCancellationDate;

    property ContractStatus: NullableString read FContractStatus write FContractStatus;
    property SaleType: NullableString read FSaleType write FSaleType;
    property CustomerPaymentDate: NullableString read FCustomerPaymentDate write FCustomerPaymentDate;
    property FinalPaymentDate: NullableString read FFinalPaymentDate write FFinalPaymentDate;
    property AccountingApprovalDate: NullableString read FAccountingApprovalDate write FAccountingApprovalDate;
    property InvitationNumber: NullableString read FInvitationNumber write FInvitationNumber;
    property RequestTimestamp: NullableString read FRequestTimestamp write FRequestTimestamp;

    property ApprovedPrice: NullableString read FApprovedPrice write FApprovedPrice;
    property PriceType: NullableString read FPriceType write FPriceType;
    property GuarantorCustomerID: NullableInt64 read FGuarantorCustomerID write FGuarantorCustomerID;
    property DiscountAmount: NullableString read FDiscountAmount write FDiscountAmount;
    property TotalAmountReceived: NullableString read FTotalAmountReceived write FTotalAmountReceived;

    property ContractFormatType: NullableString read FContractFormatType write FContractFormatType;
    property AssignorCustomerID: NullableInt64 read FAssignorCustomerID write FAssignorCustomerID;
    property AssignmentStatus: NullableString read FAssignmentStatus write FAssignmentStatus;
    property Status: NullableString read FStatus write FStatus;
    property Cancellation: NullableString read FCancellation write FCancellation;

    property Organization: NullableString read FOrganization write FOrganization;
    property KardexNumber: NullableString read FKardexNumber write FKardexNumber;
    property KardexDate: NullableString read FKardexDate write FKardexDate;
    property CancellationIBAN: NullableString read FCancellationIBAN write FCancellationIBAN;
    property TotalChecksAmount: NullableString read FTotalChecksAmount write FTotalChecksAmount;
    property NumberOfChecks: NullableString read FNumberOfChecks write FNumberOfChecks;
    property Organization1: NullableString read FOrganization1 write FOrganization1;

    property RequestCreatorUser: NullableString read FRequestCreatorUser write FRequestCreatorUser;
    property DocumentType: NullableString read FDocumentType write FDocumentType;
    property IsContractBlacklisted: NullableString read FIsContractBlacklisted write FIsContractBlacklisted;
    property IsCustomerBlacklisted: NullableString read FIsCustomerBlacklisted write FIsCustomerBlacklisted;
    property LastIssuedInvitation: NullableString read FLastIssuedInvitation write FLastIssuedInvitation;
    property LastCompletedPaymentInvitation: NullableString read FLastCompletedPaymentInvitation write FLastCompletedPaymentInvitation;

    property FirstSignatureDate: NullableString read FFirstSignatureDate write FFirstSignatureDate;
    property FirstSignatory: NullableString read FFirstSignatory write FFirstSignatory;
    property SecondSignatureDate: NullableString read FSecondSignatureDate write FSecondSignatureDate;
    property SecondSignatory: NullableString read FSecondSignatory write FSecondSignatory;

    property CustomerOutstandingBalance: NullableString read FCustomerOutstandingBalance write FCustomerOutstandingBalance;
    property ChecksCountAfterKardex: NullableString read FChecksCountAfterKardex write FChecksCountAfterKardex;
    property ScrappingCertificateDate: NullableString read FScrappingCertificateDate write FScrappingCertificateDate;

    property ContractFileID: Int64 read FContractFileID write FContractFileID;
  end;

implementation

end.

