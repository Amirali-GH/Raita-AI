unit Model.Contract;

interface

uses
  MVCFramework.Nullables,
  MVCFramework.Serializer.Commons,
  MVCFramework.ActiveRecord;

type
  [MVCTable('contract_contracts')]
  [MVCNameCase(ncLowerCase)]
  [MVCEntityActions([eaRetrieve, eaCreate, eaUpdate, eaDelete])]
  TContract = class(TMVCActiveRecord)
  private
    [MVCTableField('ID', [foPrimaryKey, foAutoGenerated])]
    FID: Int64;

    [MVCTableField('Number')] FNumber: NullableString;
    [MVCTableField('Date')] FDate: NullableString;
    [MVCTableField('Gregorian_Date')] FGregorianDate: NullableString;

    [MVCTableField('Request_Number')] FRequestNumber: NullableString;
    [MVCTableField('Request_Date')] FRequestDate: NullableString;
    [MVCTableField('Request_Gregorian_Date')] FRequestGregorianDate: NullableString;

    [MVCTableField('Customer_ID')] FCustomerID: Int64;
    [MVCTableField('Dealership_ID')] FDealershipID: Int64;
    [MVCTableField('Vehicle_Type_ID')] FVehicleTypeID: Int64;
    [MVCTableField('Color_ID')] FColorID: Int64;

    [MVCTableField('Gateway')] FGateway: NullableString;
    [MVCTableField('Region')] FRegion: NullableString;
    [MVCTableField('Sales_License_ID')] FSalesLicenseID: Int64;
    [MVCTableField('Payment_Method_ID')] FPaymentMethodID: Int64;

