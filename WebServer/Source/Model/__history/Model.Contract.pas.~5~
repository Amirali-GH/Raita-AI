    [MVCTableField('Delivery_Month')] FDeliveryMonth: NullableString;
    [MVCTableField('Delivery_Dealership_ID')] FDeliveryDealershipID: NullableInt64;
    [MVCTableField('Delivery_Location')] FDeliveryLocation: NullableString;

    [MVCTableField('Registration_Documents_Delivery_Status')] FRegistrationDocsStatus: NullableString;
    [MVCTableField('Final_Payment_Documents_Delivery_Status')] FFinalPaymentDocsStatus: NullableString;
    [MVCTableField('Invoice_Not_Issued_Status')] FInvoiceNotIssuedStatus: NullableString;
    [MVCTableField('Verification_Documents_Status')] FVerificationDocsStatus: NullableString;
    [MVCTableField('Chassis_Status')] FChassisStatus: NullableString;

    [MVCTableField('Allocation_Date')] FAllocationDate: NullableString;
    [MVCTableField('Allocation_Gregorian_Date')] FAllocationGregorianDate: NullableString;
    [MVCTableField('Plating_Date')] FPlatingDate: NullableString;
    [MVCTableField('Plating_Gregorian_Date')] FPlatingGregorianDate: NullableString;
    [MVCTableField('Plating_Gregorian_Date1')] FPlatingGregorianDate1: NullableString;

    [MVCTableField('Document_Date')] FDocumentDate: NullableString;
    [MVCTableField('Document_Status')] FDocumentStatus: NullableString;
    [MVCTableField('Physical_Delivery_Date')] FPhysicalDeliveryDate: NullableString;
    [MVCTableField('Plate_Number')] FPlateNumber: NullableString;
    [MVCTableField('Cancellation_Date')] FCancellationDate: NullableString;

    [MVCTableField('Contract_Status')] FContractStatus: NullableString;
    [MVCTableField('Sale_Type')] FSaleType: NullableString;
    [MVCTableField('Customer_Payment_Date')] FCustomerPaymentDate: NullableString;
    [MVCTableField('Final_Payment_Date')] FFinalPaymentDate: NullableString;
    [MVCTableField('Accounting_Approval_Date')] FAccountingApprovalDate: NullableString;
    [MVCTableField('Invitation_Number')] FInvitationNumber: NullableString;
    [MVCTableField('Request_Timestamp')] FRequestTimestamp: NullableString;

    [MVCTableField('Approved_Price')] FApprovedPrice: NullableString;
    [MVCTableField('Price_Type')] FPriceType: NullableString;
    [MVCTableField('Guarantor_Customer_ID')] FGuarantorCustomerID: NullableInt64;
    [MVCTableField('Discount_Amount')] FDiscountAmount: NullableString;
    [MVCTableField('Total_Amount_Received')] FTotalAmountReceived: NullableString;

    [MVCTableField('Contract_Format_Type')] FContractFormatType: NullableString;
    [MVCTableField('Assignor_Customer_ID')] FAssignorCustomerID: NullableInt64;
    [MVCTableField('Assignment_Status')] FAssignmentStatus: NullableString;
    [MVCTableField('Status')] FStatus: NullableString;
    [MVCTableField('Cancellation')] FCancellation: NullableString;

    [MVCTableField('Organization')] FOrganization: NullableString;
    [MVCTableField('Kardex_Number')] FKardexNumber: NullableString;
    [MVCTableField('Kardex_Date')] FKardexDate: NullableString;
    [MVCTableField('Cancellation_IBAN')] FCancellationIBAN: NullableString;
    [MVCTableField('Total_Checks_Amount')] FTotalChecksAmount: NullableString;
    [MVCTableField('Number_of_Checks')] FNumberOfChecks: NullableString;
    [MVCTableField('Organization1')] FOrganization1: NullableString;

