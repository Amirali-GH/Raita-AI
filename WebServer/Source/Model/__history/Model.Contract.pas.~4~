unit Model.Contract;

interface

uses
  MVCFramework.Nullables,
  MVCFramework.Serializer.Commons,
  MVCFramework.ActiveRecord;

type
  [MVCTable('contract_contracts')]
  [MVCNameCase(ncLowerCase)]
  [MVCEntityActions([eaRetrieve, eaCreate, eaUpdate, eaDelete])]
  TContract = class(TMVCActiveRecord)
  private
    [MVCTableField('ID', [foPrimaryKey, foAutoGenerated])]
    FID: Int64;

    [MVCTableField('Number')] FNumber: NullableString;
    [MVCTableField('Date')] FDate: NullableString;
    [MVCTableField('Gregorian_Date')] FGregorianDate: NullableString;

    [MVCTableField('Request_Number')] FRequestNumber: NullableString;
    [MVCTableField('Request_Date')] FRequestDate: NullableString;
    [MVCTableField('Request_Gregorian_Date')] FRequestGregorianDate: NullableString;

    [MVCTableField('Customer_ID')] FCustomerID: Int64;
    [MVCTableField('Dealership_ID')] FDealershipID: Int64;
    [MVCTableField('Vehicle_Type_ID')] FVehicleTypeID: Int64;
    [MVCTableField('Color_ID')] FColorID: Int64;

    [MVCTableField('Gateway')] FGateway: NullableString;
    [MVCTableField('Region')] FRegion: NullableString;
    [MVCTableField('Sales_License_ID')] FSalesLicenseID: Int64;
    [MVCTableField('Payment_Method_ID')] FPaymentMethodID: Int64;

    [MVCTableField('Delivery_Date')] FDeliveryDate: NullableString;
    [MVCTableField('Delivery_Due_Days')] FDeliveryDueDays: NullableString;
    [MVCTableField('Assignable_Chassis_10th_Digit')] FAssignableChassis10thDigit: NullableString;
    [MVCTableField('Chassis_Number')] FChassisNumber: NullableString;
    [MVCTableField('Delivery_Month')] FDeliveryMonth: NullableString;
    [MVCTableField('Delivery_Dealership_ID')] FDeliveryDealershipID: NullableInt64;
    [MVCTableField('Delivery_Location')] FDeliveryLocation: NullableString;

    [MVCTableField('Registration_Documents_Delivery_Status')] FRegistrationDocsStatus: NullableString;
    [MVCTableField('Final_Payment_Documents_Delivery_Status')] FFinalPaymentDocsStatus: NullableString;
    [MVCTableField('Invoice_Not_Issued_Status')] FInvoiceNotIssuedStatus: NullableString;
    [MVCTableField('Verification_Documents_Status')] FVerificationDocsStatus: NullableString;
    [MVCTableField('Chassis_Status')] FChassisStatus: NullableString;

    [MVCTableField('Allocation_Date')] FAllocationDate: NullableString;
    [MVCTableField('Allocation_Gregorian_Date')] FAllocationGregorianDate: NullableString;
    [MVCTableField('Plating_Date')] FPlatingDate: NullableString;
    [MVCTableField('Plating_Gregorian_Date')] FPlatingGregorianDate: NullableString;
    [MVCTableField('Plating_Gregorian_Date1')] FPlatingGregorianDate1: NullableString;

    [MVCTableField('Document_Date')] FDocumentDate: NullableString;
    [MVCTableField('Document_Status')] FDocumentStatus: NullableString;
    [MVCTableField('Physical_Delivery_Date')] FPhysicalDeliveryDate: NullableString;
    [MVCTableField('Plate_Number')] FPlateNumber: NullableString;
    [MVCTableField('Cancellation_Date')] FCancellationDate: NullableString;

    [MVCTableField('Contract_Status')] FContractStatus: NullableString;
    [MVCTableField('Sale_Type')] FSaleType: NullableString;
    [MVCTableField('Customer_Payment_Date')] FCustomerPaymentDate: NullableString;
    [MVCTableField('Final_Payment_Date')] FFinalPaymentDate: NullableString;
    [MVCTableField('Accounting_Approval_Date')] FAccountingApprovalDate: NullableString;
    [MVCTableField('Invitation_Number')] FInvitationNumber: NullableString;
    [MVCTableField('Request_Timestamp')] FRequestTimestamp: NullableString;

    [MVCTableField('Approved_Price')] FApprovedPrice: NullableString;
    [MVCTableField('Price_Type')] FPriceType: NullableString;
    [MVCTableField('Guarantor_Customer_ID')] FGuarantorCustomerID: NullableInt64;
    [MVCTableField('Discount_Amount')] FDiscountAmount: NullableString;
    [MVCTableField('Total_Amount_Received')] FTotalAmountReceived: NullableString;

    [MVCTableField('Contract_Format_Type')] FContractFormatType: NullableString;
    [MVCTableField('Assignor_Customer_ID')] FAssignorCustomerID: NullableInt64;
    [MVCTableField('Assignment_Status')] FAssignmentStatus: NullableString;
    [MVCTableField('Status')] FStatus: NullableString;
    [MVCTableField('Cancellation')] FCancellation: NullableString;

    [MVCTableField('Organization')] FOrganization: NullableString;
    [MVCTableField('Kardex_Number')] FKardexNumber: NullableString;
    [MVCTableField('Kardex_Date')] FKardexDate: NullableString;
    [MVCTableField('Cancellation_IBAN')] FCancellationIBAN: NullableString;
    [MVCTableField('Total_Checks_Amount')] FTotalChecksAmount: NullableString;
    [MVCTableField('Number_of_Checks')] FNumberOfChecks: NullableString;
    [MVCTableField('Organization1')] FOrganization1: NullableString;

    [MVCTableField('Request_Creator_User')] FRequestCreatorUser: NullableString;
    [MVCTableField('Document_Type')] FDocumentType: NullableString;
    [MVCTableField('Is_Contract_Blacklisted')] FIsContractBlacklisted: NullableString;
    [MVCTableField('Is_Customer_Blacklisted')] FIsCustomerBlacklisted: NullableString;
    [MVCTableField('Last_Issued_Invitation')] FLastIssuedInvitation: NullableString;
    [MVCTableField('Last_Completed_Payment_Invitation')] FLastCompletedPaymentInvitation: NullableString;

    [MVCTableField('First_Signature_Date')] FFirstSignatureDate: NullableString;
    [MVCTableField('First_Signatory')] FFirstSignatory: NullableString;
    [MVCTableField('Second_Signature_Date')] FSecondSignatureDate: NullableString;
    [MVCTableField('Second_Signatory')] FSecondSignatory: NullableString;

    [MVCTableField('Customer_Outstanding_Balance')] FCustomerOutstandingBalance: NullableString;
    [MVCTableField('Checks_Count_After_Kardex')] FChecksCountAfterKardex: NullableString;
    [MVCTableField('Scrapping_Certificate_Date')] FScrappingCertificateDate: NullableString;

    [MVCTableField('ContractFileID')] FContractFileID: Int64;

  public
    property ID: Int64 read FID write FID;

    property Number: NullableString read FNumber write FNumber;
    property Date_: NullableString read FDate write FDate;
    property GregorianDate: NullableString read FGregorianDate write FGregorianDate;

    property RequestNumber: NullableString read FRequestNumber write FRequestNumber;
    property RequestDate: NullableString read FRequestDate write FRequestDate;
    property RequestGregorianDate: NullableString read FRequestGregorianDate write FRequestGregorianDate;

    property CustomerID: Int64 read FCustomerID write FCustomerID;
    property DealershipID: Int64 read FDealershipID write FDealershipID;
    property VehicleTypeID: Int64 read FVehicleTypeID write FVehicleTypeID;
    property ColorID: Int64 read FColorID write FColorID;

    property Gateway: NullableString read FGateway write FGateway;
    property Region: NullableString read FRegion write FRegion;
    property SalesLicenseID: Int64 read FSalesLicenseID write FSalesLicenseID;
    property PaymentMethodID: Int64 read FPaymentMethodID write FPaymentMethodID;

    property DeliveryDate: NullableString read FDeliveryDate write FDeliveryDate;
    property DeliveryDueDays: NullableString read FDeliveryDueDays write FDeliveryDueDays;
    property AssignableChassis10thDigit: NullableString read FAssignableChassis10thDigit write FAssignableChassis10thDigit;
    property ChassisNumber: NullableString read FChassisNumber write FChassisNumber;
    property DeliveryMonth: NullableString read FDeliveryMonth write FDeliveryMonth;
    property DeliveryDealershipID: NullableInt64 read FDeliveryDealershipID write FDeliveryDealershipID;
    property DeliveryLocation: NullableString read FDeliveryLocation write FDeliveryLocation;

    // ادامه پراپرتی‌ها همین‌طور مطابق Private...
    property ContractFileID: Int64 read FContractFileID write FContractFileID;
  end;

implementation

end.

